<section class="content-header"><%= breadcrumb({ breadcrumbTitle: "Hardware Utilization" }) %></section>

<section class="content">
<div class="box">
    <div class="box-body">
        <table id="hardware-table" class="table table-bordered table-striped" style='vertical-align:middle'>
        <thead>
            <tr>
                <th>Machine Name</th>
                <th>IP Address</th>
                <th style='width:80px; text-align:center'>CPU</th>
                <th style='width:80px; text-align:center'>Memory</th>
                <th style='width:80px; text-align:center'>GPU</th>
                <th style='width:80px; text-align:center'>GPU Mem</th>
                <th style='width:80px; text-align:center'>Disk</th>
                <th style='width:80px; text-align:center'>Ethernet</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i = 0; i < 160; i ++) { %>
            <tr>
                <td><a href="#">Node <%= i + 1 %></a></td>
                <td>192.168.1.<%= i + 1 %></td>
                <td style='text-align:center'><i class="fa fa-circle text-red" title="3"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
            </tr>
            <% } %>
            <tr>
                <td><a href="#">Node A in Seattle West</a></td>
                <td>192.168.1.201</td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-red" title="3"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
            </tr>
            <tr>
                <td><a href="#">Node B in Seattle East</a></td>
                <td>192.168.1.202</td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-red" title="3"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-red" title="3"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
            </tr>
            <tr>
                <td><a href="#">Node C in New York West</a></td>
                <td>192.168.1.203</td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
                <td style='text-align:center'><i class="fa fa-circle-thin text-grey" title="0"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-green" title="1"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-red" title="3"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
                <td style='text-align:center'><i class="fa fa-circle text-yellow" title="2"></i></td>
            </tr>
        </tbody>
        </table>
    </div>
</section>

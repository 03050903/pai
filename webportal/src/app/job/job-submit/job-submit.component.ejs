<section class="content-header"><%= breadcrumb({ breadcrumbTitle: "Submit Jobs" }) %></section>

<section class="content">
  <div class="box box-primary">
    <div class="box-body">
      <div class="row">
        <div class="form-group col-md-2">
          <label for="fileUpload">Import Json</label>
          <input id="fileUpload" type="file">
        </div>
        <div class="form-group col-md-2">
          <label for="submitJob">Submit Job</label>
          <input id="submitJob" type="submit">
          <p id="submitHint" class="help-block"></p>
        </div>
      </div>
    </div>
  </div>
  <div class="box">
    <div class="box-body" id="editor-holder">
      
    </div>
  </div>
  <%= loading() %>
</section>